<?xml version="1.0" encoding="utf-8" ?> 
<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense" xmlns:shell="http://www.sitecore.net/shell">
  <Gallery.Versions>
    <Gallery>
      <CodeBeside Type="Examples.ContentEditorCulture.Shell.Applications.Content_Manager.Galleries.Versions.GalleryVersionsForm,Examples"/>

      <Stylesheet Key="GalleryLanguages">
        .scMenuPanelItem, 
        .scMenuPanelItem_Hover, 
        .scMenuPanelItemSelected_Hover, 
        .scMenuPanelItemSelected {
        padding-left: 0;
        padding-right: 0;
        }
        .scGalleryMenuLine {
        position: absolute;
        bottom: 12px;
        left: 1px;
        right: 1px;
        height: 2px;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
        }
        .scGalleryGrip {
        position: absolute;
        bottom: 1px;
        left: 1px;
        right: 1px;
        height: 10px;
        }
        
        #Versions{
        height: auto !important;
        }
        
      </Stylesheet>

      <div class="scStretch">
        <div class="scGalleryContent13">
          <GalleryMenu ID="Options" Class="scStretch">

            <MenuPanel Height="100%">
              <Scrollbox ID="Versions" Class="scScrollbox" />
            </MenuPanel>

          </GalleryMenu>
        </div>
        <div class="scGalleryBottom13">
          <GalleryMenuLine />
          <Gallery.Grip />
        </div>
      </div>
      
    </Gallery>
  </Gallery.Versions>
</control>